{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "@": [
                "src"
            ],
            "@/*": [
                "src/*"
            ],
            "@assets": [
                "@\\assets"
            ],
            "@assets/*": [
                "@\\assets/*"
            ],
            "@fonts": [
                "@assets\\fonts"
            ],
            "@fonts/*": [
                "@assets\\fonts/*"
            ],
            "@img": [
                "@assets\\img"
            ],
            "@img/*": [
                "@assets\\img/*"
            ],
            "@scss": [
                "@assets\\scss"
            ],
            "@scss/*": [
                "@assets\\scss/*"
            ],
            "@scss-ui": [
                "@scss\\ui"
            ],
            "@scss-ui/*": [
                "@scss\\ui/*"
            ],
            "@scss-blocks": [
                "@scss\\blocks"
            ],
            "@scss-blocks/*": [
                "@scss\\blocks/*"
            ],
            "@scss-libs": [
                "@scss\\libs"
            ],
            "@scss-libs/*": [
                "@scss\\libs/*"
            ],
            "@scss-general": [
                "@scss\\general"
            ],
            "@scss-general/*": [
                "@scss\\general/*"
            ],
            "@scss-func": [
                "@scss\\_scss-func"
            ],
            "@scss-func/*": [
                "@scss\\_scss-func/*"
            ],
            "@js": [
                "@assets\\js"
            ],
            "@js/*": [
                "@assets\\js/*"
            ],
            "@js-moduls": [
                "@js\\moduls"
            ],
            "@js-moduls/*": [
                "@js\\moduls/*"
            ],
            "@js-base-func": [
                "@js\\base-func"
            ],
            "@js-base-func/*": [
                "@js\\base-func/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}