            "@burger-button-render-scss/*": [
                "@components\\ui\\burger-button\\assets\\scss\\burger-button-render.scss/*"
            ],
            "@burger-button-interaction-scss": [
                "@components\\ui\\burger-button\\assets\\scss\\burger-button-interaction.scss"
            ],
            "@burger-button-interaction-scss/*": [
                "@components\\ui\\burger-button\\assets\\scss\\burger-button-interaction.scss/*"
            ],
            "@burger-button-fonts": [
                "@components\\ui\\burger-button\\assets\\fonts"
            ],
            "@burger-button-fonts/*": [
                "@components\\ui\\burger-button\\assets\\fonts/*"
            ],
            "@burger-button-img": [
                "@components\\ui\\burger-button\\assets\\img"
            ],
            "@burger-button-img/*": [
                "@components\\ui\\burger-button\\assets\\img/*"
            ],
            "@scroll-to-top-button": [
                "@components\\ui\\scroll-to-top-button"
            ],
            "@scroll-to-top-button/*": [
                "@components\\ui\\scroll-to-top-button/*"
            ],
            "@scroll-to-top-button-main-js": [
                "@components\\ui\\scroll-to-top-button\\assets\\js\\scroll-to-top-button-main.js"
            ],
            "@scroll-to-top-button-main-js/*": [
                "@components\\ui\\scroll-to-top-button\\assets\\js\\scroll-to-top-button-main.js/*"
            ],
            "@scroll-to-top-button-render-scss": [
                "@components\\ui\\scroll-to-top-button\\assets\\scss\\scroll-to-top-button-render.scss"
            ],
            "@scroll-to-top-button-render-scss/*": [
                "@components\\ui\\scroll-to-top-button\\assets\\scss\\scroll-to-top-button-render.scss/*"
            ],
            "@scroll-to-top-button-interaction-scss": [
                "@components\\ui\\scroll-to-top-button\\assets\\scss\\scroll-to-top-button-interaction.scss"
            ],
            "@scroll-to-top-button-interaction-scss/*": [
                "@components\\ui\\scroll-to-top-button\\assets\\scss\\scroll-to-top-button-interaction.scss/*"
            ],
            "@scroll-to-top-button-fonts": [
                "@components\\ui\\scroll-to-top-button\\assets\\fonts"
            ],
            "@scroll-to-top-button-fonts/*": [
                "@components\\ui\\scroll-to-top-button\\assets\\fonts/*"
            ],
            "@scroll-to-top-button-img": [
                "@components\\ui\\scroll-to-top-button\\assets\\img"
            ],
            "@scroll-to-top-button-img/*": [
                "@components\\ui\\scroll-to-top-button\\assets\\img/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}