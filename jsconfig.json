{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "@": [
                "src"
            ],
            "@/*": [
                "src/*"
            ],
            "@assets": [
                "@\\assets"
            ],
            "@assets/*": [
                "@\\assets/*"
            ],
            "@fonts": [
                "@assets\\fonts"
            ],
            "@fonts/*": [
                "@assets\\fonts/*"
            ],
            "@img": [
                "@assets\\img"
            ],
            "@img/*": [
                "@assets\\img/*"
            ],
            "@scss": [
                "@assets\\scss"
            ],
            "@scss/*": [
                "@assets\\scss/*"
            ],
            "@scss_moduls": [
                "@scss\\moduls"
            ],
            "@scss_moduls/*": [
                "@scss\\moduls/*"
            ],
            "@js": [
                "@assets\\js"
            ],
            "@js/*": [
                "@assets\\js/*"
            ],
            "@js_moduls": [
                "@js\\moduls"
            ],
            "@js_moduls/*": [
                "@js\\moduls/*"
            ],
            "@js_base_func": [
                "@js\\base_func"
            ],
            "@js_base_func/*": [
                "@js\\base_func/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}