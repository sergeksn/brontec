{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "@": [
                "src"
            ],
            "@/*": [
                "src/*"
            ],
            "@components": [
                "@\\components"
            ],
            "@components/*": [
                "@\\components/*"
            ],
            "@components-blocks": [
                "@components\\blocks"
            ],
            "@components-blocks/*": [
                "@components\\blocks/*"
            ],
            "@assets": [
                "@\\assets"
            ],
            "@assets/*": [
                "@\\assets/*"
            ],
            "@fonts": [
                "@assets\\fonts"
            ],
            "@fonts/*": [
                "@assets\\fonts/*"
            ],
            "@img": [
                "@assets\\img"
            ],
            "@img/*": [
                "@assets\\img/*"
            ],
            "@scss": [
                "@assets\\scss"
            ],
            "@scss/*": [
                "@assets\\scss/*"
            ],
            "@scss-ui": [
                "@scss\\ui"
            ],
            "@scss-ui/*": [
                "@scss\\ui/*"
            ],
            "@scss-blocks": [
                "@scss\\blocks"
            ],
            "@scss-blocks/*": [
                "@scss\\blocks/*"
            ],
            "@scss-libs": [
                "@scss\\libs"
            ],
            "@scss-libs/*": [
                "@scss\\libs/*"
            ],
            "@scss-general": [
                "@scss\\general"
            ],
            "@scss-general/*": [
                "@scss\\general/*"
            ],
            "@scss-func": [
                "@scss\\_scss-func"
            ],
            "@scss-func/*": [
                "@scss\\_scss-func/*"
            ],
            "@js": [
                "@assets\\js"
            ],
            "@js/*": [
                "@assets\\js/*"
            ],
            "@js-moduls": [
                "@js\\moduls"
            ],
            "@js-moduls/*": [
                "@js\\moduls/*"
            ],
            "@js-base-func": [
                "@js\\base-func"
            ],
            "@js-base-func/*": [
                "@js\\base-func/*"
            ],
            "@header": [
                "@components\\blocks\\header"
            ],
            "@header/*": [
                "@components\\blocks\\header/*"
            ],
            "@header-main-js": [
                "@components\\blocks\\header\\assets\\js\\header-main.js"
            ],
            "@header-main-js/*": [
                "@components\\blocks\\header\\assets\\js\\header-main.js/*"
            ],
            "@header-render-scss": [
                "@components\\blocks\\header\\assets\\scss\\header-render.scss"
            ],
            "@header-render-scss/*": [
                "@components\\blocks\\header\\assets\\scss\\header-render.scss/*"
            ],
            "@header-interaction-scss": [
                "@components\\blocks\\header\\assets\\scss\\header-interaction.scss"
            ],
            "@header-interaction-scss/*": [
                "@components\\blocks\\header\\assets\\scss\\header-interaction.scss/*"
            ],
            "@header-fonts": [
                "@components\\blocks\\header\\assets\\fonts"
            ],
            "@header-fonts/*": [
                "@components\\blocks\\header\\assets\\fonts/*"
            ],
            "@header-img": [
                "@components\\blocks\\header\\assets\\img"
            ],
            "@header-img/*": [
                "@components\\blocks\\header\\assets\\img/*"
            ],
            "@burger-button": [
                "@components\\ui\\burger-button"
            ],
            "@burger-button/*": [
                "@components\\ui\\burger-button/*"
            ],
            "@burger-button-main-js": [
                "@components\\ui\\burger-button\\assets\\js\\burger-button-main.js"
            ],
            "@burger-button-main-js/*": [
                "@components\\ui\\burger-button\\assets\\js\\burger-button-main.js/*"
            ],
            "@burger-button-render-scss": [
                "@components\\ui\\burger-button\\assets\\scss\\burger-button-render.scss"
